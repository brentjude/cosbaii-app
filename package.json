{
  "name": "cosbaii-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.development -- next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "prisma:studio:dev": "dotenv -e .env.development -- npx prisma studio",
    "prisma:studio:prod": "dotenv -e .env.production -- npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "prisma:migrate:prod": "dotenv -e .env.production -- npx prisma migrate deploy",
    "prisma:push:dev": "dotenv -e .env.development -- npx prisma db push",
    "prisma:push:prod": "dotenv -e .env.production -- npx prisma db push",
    "prisma:seed:dev": "dotenv -e .env.development -- npx prisma db seed",
    "prisma:seed:prod": "dotenv -e .env.production -- npx prisma db seed",
    "prisma:reset:dev": "dotenv -e .env.development -- npx prisma migrate reset",
    "db:pull:dev": "dotenv -e .env.development -- npx prisma db pull",
    "db:pull:prod": "dotenv -e .env.production -- npx prisma db pull"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.13.0",
    "@prisma/nextjs-monorepo-workaround-plugin": "^6.17.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "date-fns": "^4.1.0",
    "jose": "^6.0.12",
    "motion": "^12.23.24",
    "next": "15.4.4",
    "next-auth": "^4.24.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "resend": "^6.0.1",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "daisyui": "^5.0.48",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.37.0",
    "eslint-config-next": "15.4.4",
    "prisma": "^6.13.0",
    "save": "^2.9.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
